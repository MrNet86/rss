<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.vnpt.portlet.user.model.PermissionType" />
	<class name="com.vnpt.portlet.user.model.impl.PermissionTypeImpl" table="eportal_permission_type">
		<cache usage="read-write" />
		<id name="permissionTypeId" type="long">
			<generator class="increment" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="typeName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="typeCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="isActive" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>