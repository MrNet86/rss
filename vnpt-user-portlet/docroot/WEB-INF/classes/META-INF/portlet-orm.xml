			<basic name="description" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<entity class="com.vnpt.portlet.user.model.impl.PermissionGroupImpl" name="PermissionGroup">
		<table name="eportal_permission_group" />
		<attributes />
	</entity>
	<entity class="com.vnpt.portlet.user.model.impl.PermissionTypeImpl" name="PermissionType">
		<table name="eportal_permission_type" />
